%table(class="table table-hover text-center" )
  %tr
    %td
      编号
    %td
      用户名
    %td
      操作
  -@users.each do |user|
    %tr
      %td
        =@count+=1
      %td
        =user.name
      %td
        =link_to "删除",manager_logined_path(id:user.id,:page=>params[:page]),:confirm => "确认删除吗？"
        |
        =link_to "修改密码",edit_user_path(:id=>user.id)